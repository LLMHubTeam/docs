{
  "openapi": "3.1.0",
  "info": {
    "title": "Completion",
    "version": "1.0.0"
  },
  "paths": {
    "/completions": {
      "post": {
        "summary": "Send a completion request to a selected model (text-only format)",
        "operationId": "completion",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "model": {
                    "type": "string"
                  },
                  "prompt": {
                    "type": "string"
                  }
                },
                "required": ["model", "prompt"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response"
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "servers": [
    {
      "url": "https://openrouter.ai"
    }
  ]
}
